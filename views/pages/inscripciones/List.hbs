{{> menu}}

<h2 style="text-align: center;">{{ viewTitle }}</h2>
<div class="container">
    <table class="table table-striped table-dark">
        <thead>
            <th>Estudiante ID</th>
            <th>Curso ID</th>
            <th>Fecha</th>
        </thead>
        <tbody>
            {{#each list}}
            <tr>
                <td>{{this.studentId}}</td>
                <td>{{this.courseId}}</td>
                <td>{{this.date}}</td>
                <td>
                    <a href="/inscripciones/{{this._id}}" class="btn btn-info">Actualizar</a>
                    <a href="/inscripciones/delete/{{this._id}}" class="btn btn-danger"
                        onclick="return confirm('Â¿Desea eliminar este registro?');">
                        Borrar</a>

                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>